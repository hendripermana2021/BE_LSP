{
  "name": "be-smom",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize db:create",
    "dev": "nodemon ./index.js",
    "start": "node ./index.js",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:restart": "npx sequelize-cli db:drop db:create db:migrate db:seed",
    "tbl_user": "npx sequelize-cli model:generate --name tbl_user --attributes name_user:string,sex:string,email:string,password:string,real_password:string,role_id:string,accesstoken:text,refreshtoken:text",
    "tbl_role": "npx sequelize-cli model:generate --name tbl_role --attributes role:string",
    "tbl_stuff": "npx sequelize-cli model:generate --name tbl_stuff --attributes name_stuff:string,price:double,type_stuff:integer,disc:double,user_id:integer,image:string,qty:integer",
    "tbl_type_stuff": "npx sequelize-cli model:generate --name tbl_type_stuff --attributes name_type:string",
    "seed-user": "npx sequelize-cli seed:generate --name demo-tbl_user",
    "seed-role": "npx sequelize-cli seed:generate --name demo-tbl_role",
    "seed-stuff": "npx sequelize-cli seed:generate --name demo-tbl_stuff",
    "seed-type-stuff": "npx sequelize-cli seed:generate --name demo-tbl_type_stuff"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^9.1.3",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.0",
    "nodemon": "^3.1.3",
    "path": "^0.12.7",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  },
  "description": ""
}
